  "scripts": {
    "start-app-lab-desktop": "yarn workspace @bcmi-labs/app-lab-desktop wails-dev",
    "eslint-fix": "eslint './**' --fix",
    "test": "lerna run test --stream",
    "coverage": "lerna run coverage --stream",
    "tokens:pull": "yarn workspace @bcmi-labs/cloud-editor-dev-config tokens:pull",
    "themes:generate": "yarn workspace @bcmi-labs/cloud-editor-dev-config themes:generate",
    "i18n:extract": "formatjs extract 'ui-packages/**/*.ts*' --ignore='**/*.d.ts' --out-file app/core-ui/lang/en.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile-folder --ast app/core-ui/lang app/core-ui/lang/compiled",
    "i18n:generate": "yarn i18n:extract && yarn i18n:compile",
    "openapi-types:generate": "yarn workspace @bcmi-labs/cloud-editor-dev-config openapi-types:generate",
    "snyk-scan": "snyk test --strict-out-of-sync=false --yarn-workspaces --json-file-output=vuln.json",
    "bump-major": "lerna version major --no-push --no-git-tag-version --force-publish='*'",
    "bump-minor": "lerna version minor --no-push --no-git-tag-version --force-publish='*'",
    "bump-patch": "lerna version patch --no-push --no-git-tag-version --force-publish='*'",
    "audit-deps": "yarn npm audit --all",
    "build:arduino-app-lab-desktop": "yarn workspace @bcmi-labs/app-lab-desktop wails-build",
    "build:arduino-app-lab:deb": "docker build --platform=linux/arm64 --secret id=npm_token_id,env=NPM_TOKEN --build-arg BINARY_NAME=arduino-app-lab --build-arg VERSION=0.0.0-$(git rev-parse --short HEAD) --build-arg ARCH=arm64 --build-arg DEB_NAME=arduino-app-lab -f standalone-apps/app-lab-desktop/build/debian/Dockerfile --output ./build .",
    "install:arduino-app-lab:deb": "adb push ./build/arduino-app-lab_0.0.0-$(git rev-parse --short HEAD)-1_arm64.deb /tmp/arduino-app-lab_0.0.0-$(git rev-parse --short HEAD)-1_arm64.deb && adb shell dpkg -i /tmp/arduino-app-lab_0.0.0-$(git rev-parse --short HEAD)-1_arm64.deb && adb shell rm /tmp/arduino-app-lab_0.0.0-$(git rev-parse --short HEAD)-1_arm64.deb",
    "licensed:update:arduino-app-lab-desktop": "yarn workspace @bcmi-labs/al-license licensed",
    "fetch-learn:arduino-app-lab-desktop": "yarn workspace @bcmi-labs/al-learn-contents fetch",
    "add-learn-lastmod:arduino-app-lab-desktop": "yarn workspace @bcmi-labs/al-learn-contents add-lastmod"
  },
  "packageManager": "yarn@3.5.0",
  "license": "GPL-3.0"
}
