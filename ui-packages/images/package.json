{
  "name": "@bcmi-labs/cloud-editor-images",
  "version": "1.4.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "buildDev": "tsc && vite build --mode development",
    "watch": "yarn buildDev --watch"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted",
    "module": "./dist/lib/images.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/lib/images.es.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:bcmi-labs/cloud-editor-mono.git"
  },
  "module": "./lib/index.ts",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.ts"
    },
    "./public/arduino-tab-logo.svg": {
      "import": "./public/arduino-tab-logo.svg"
    },
    "./public/arduino-tab-logo-white.svg": {
      "import": "./public/arduino-tab-logo-white.svg"
    },
    "./assets/icons": {
      "import": "./assets/icons/index.ts"
    },
    "./assets/images": {
      "import": "./assets/images/index.tsx"
    },
    "./assets/devices": {
      "import": "./assets/devices/index.ts"
    },
    "./assets/learn": {
      "import": "./assets/learn/index.ts"
    }
  }
}
