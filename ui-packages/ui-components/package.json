{
  "name": "@bcmi-labs/cloud-editor-ui-components",
  "version": "1.4.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "buildDev": "tsc && vite build --mode development",
    "watch": "yarn buildDev --watch",
    "storybook-init": "yarn exec sb init",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "@codemirror/autocomplete": ">= 6.9.1",
    "@codemirror/commands": ">= 6.1.1",
    "@codemirror/lang-cpp": ">= 6.0.1",
    "@codemirror/lang-python": ">= 6.2.1",
    "@codemirror/lang-yaml": ">= 6.1.2",
    "@codemirror/language": ">= 6.0.0",
    "@codemirror/merge": ">= 6.8.0",
    "@codemirror/search": ">= 6.2.2",
    "@codemirror/state": ">= 6.1.2",
    "@codemirror/view": ">= 6.3.0",
    "@tanstack/react-location": ">= 3.7.4",
    "clsx": ">= 1.2.1",
    "dompurify": ">= 3.2.5",
    "emoji-picker-react": ">= 4.14.1",
    "html-react-parser": ">=4.2.2",
    "react": ">= 18.2.0",
    "react-aria": ">= 3.20.0",
    "react-dom": ">= 18.2.0",
    "react-joyride": ">= 2.7.1",
    "react-markdown": ">= 8.0.6",
    "react-modal": ">= 3.16.1",
    "react-split": ">= 2.0.14",
    "react-split-grid": ">= 1.0.4",
    "react-use": ">= 17.4.0",
    "react-virtualized": ">= 9.22.5",
    "rehype-raw": ">= 7.0.0",
    "remark-remove-comments": ">= 1.1.1"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted",
    "module": "./dist/lib/ui-components.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/lib/ui-components.es.js"
      },
      "./dist/style.css": {
        "import": "./dist/style.css"
      },
      "./dist/shared.module.scss": {
        "import": "./dist/shared.module.scss"
      },
      "./lib/keywords": {
        "import": "./lib/keywords"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:bcmi-labs/cloud-editor-mono.git"
  },
  "module": "./lib/index.ts",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.ts"
    },
    "./lib/components-by-app/app-lab": {
      "import": "./lib/components-by-app/app-lab/index.ts"
    },
    "./lib/components-by-app/shared": {
      "import": "./lib/components-by-app/shared/index.ts"
    },
    "./public/shared.module.scss": {
      "import": "./public/shared.module.scss"
    },
    "./public/index.scss": {
      "import": "./public/index.scss"
    },
    "./lib/keywords": {
      "import": "./lib/keywords"
    },
    "./lib/sidenav/sections/settings/settings.type": {
      "import": "./lib/sidenav/sections/settings/settings.type.ts"
    },
    "./themes/theme.type": {
      "import": "./themes/theme.type.ts"
    }
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@tanstack/react-query": "4.12.0",
    "babel-loader": "8.2.5"
  }
}
