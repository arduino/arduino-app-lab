    "module": "./dist/lib/web-board-communication.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/lib/web-board-communication.es.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:bcmi-labs/cloud-editor-mono.git"
  },
  "module": "./lib/index.ts",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.ts"
    }
  }
